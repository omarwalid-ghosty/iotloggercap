## Foreground / Background switching
omar@omar-VirtualBox:~$ while true; do echo "Polling sensor..."; sleep 3; done
Polling sensor...
Polling sensor...
Polling sensor...
Polling sensor...
Polling sensor...
^Z
[2]+  Stopped                 sleep 3
omar@omar-VirtualBox:~$ bg %1
bash: bg: job 1 already in background
omar@omar-VirtualBox:~$ fg %1
while true; do
    echo "Polling sensor... $(date)" >> ~/iot_logger/logs/sensor_poll.log; sleep 3;
done
^Z
[1]+  Stopped                 while true; do
    echo "Polling sensor... $(date)" >> ~/iot_logger/logs/sensor_poll.log; sleep 3;
done
omar@omar-VirtualBox:~$ jobs
[1]+  Stopped                 while true; do
    echo "Polling sensor... $(date)" >> ~/iot_logger/logs/sensor_poll.log; sleep 3;
done
[2]-  Stopped                 sleep 3

## Kill the process (if needed)

omar@omar-VirtualBox:~$ while true; do   echo "Polling sensor... $(date)" >> ~/iot_logger/logs/sensor_poll.log;   sleep 3; done &
[3] 5317
omar@omar-VirtualBox:~$ kill 5317
omar@omar-VirtualBox:~$ jobs
[1]+  Stopped                 while true; do
    echo "Polling sensor... $(date)" >> ~/iot_logger/logs/sensor_poll.log; sleep 3;
done
[2]-  Stopped                 sleep 3
[3]   Terminated              while true; do
    echo "Polling sensor... $(date)" >> ~/iot_logger/logs/sensor_poll.log; sleep 3;
done
omar@omar-VirtualBox:~$ 
